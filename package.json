{
  "name": "kamihime-database",
  "version": "2.7.2",
  "description": "Website + API for Kamihime Database (Eros)",
  "main": "./build/index.js",
  "typings": "./src/typings",
  "scripts": {
    "build": "yarn run lint && gulp",
    "compile": "tsc -p .",
    "dev:build": "gulp build",
    "dev:src-watch": "yarn run compile --watch",
    "dev:start": "nodemon ./build/index.js --config .nodemon.json",
    "docs:build": "apidoc -f .ts -i src/routes/api/ -o ../kamihime-database-docs",
    "lint-fix": "yarn run lint --fix",
    "lint": "tslint -c .tslint.js -p .",
    "pm2": "pm2 start .pm2.yml --env production",
    "scenarios-v": "yarn run scenarios --verbose",
    "scenarios": "node build/util/scenarios",
    "test": "yarn run lint && yarn run compile"
  },
  "engine": {
    "node": ">=10"
  },
  "author": "gazmull",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazmull/kamihime-database.git"
  },
  "bugs": {
    "url": "https://github.com/gazmull/kamihime-database/issues"
  },
  "homepage": "https://github.com/gazmull/kamihime-database#readme",
  "license": "MIT",
  "dependencies": {
    "@discordjs/uws": "11.149.1",
    "anchorme": "^1.1.2",
    "canvas": "2.3.1",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "discord.js": "github:discordjs/discord.js",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "fuse.js": "^3.4.4",
    "helmet": "^3.16.0",
    "infobox-parser": "^3.2.0",
    "knex": "^0.16.3",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.3.0",
    "nodemw": "gazmull/nodemw#7a41bd4",
    "pug": "^2.0.3",
    "shortid": "^2.2.14",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/helmet": "^0.0.43",
    "@types/howler": "^2.1.0",
    "@types/jquery": "^3.3.29",
    "@types/js-cookie": "^2.2.1",
    "@types/node": "^11.11.3",
    "@types/node-fetch": "^2.1.6",
    "@types/serve-favicon": "^2.2.30",
    "@types/shortid": "^0.0.29",
    "apidoc": "^0.17.7",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.1.7",
    "gulp-typescript": "^5.0.1",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333"
  }
}
