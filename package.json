{
  "name": "kamihime-database",
  "version": "2.0.0",
  "description": "Website + API for Kamihime Database (Eros)",
  "main": "./build/index.js",
  "typings": "./src/typings",
  "scripts": {
    "test": "npm run lint && npm run compile && npm run finalize",
    "compile": "tsc -p .",
    "lint": "tslint -p .",
    "finalize": "npm run uglify && npm run del-proto && echo --== transpile done ==--",
    "serve": "npm run test && node .",
    "pm2-serve": "npm run test && pm2 start .pm2.yml --env production",
    "uglify": "uglifyjs-folder build/proto --config-file uglify.json -ey -x .js -o build",
    "del-proto": "del-cli build/proto"
  },
  "engine": {
    "node": ">=10"
  },
  "author": "gazmull",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazmull/kamihime-database.git"
  },
  "bugs": {
    "url": "https://github.com/gazmull/kamihime-database/issues"
  },
  "homepage": "https://github.com/gazmull/kamihime-database#readme",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "canvas": "^2.1.0",
    "compression": "^1.7.3",
    "discord.js": "github:discordjs/discord.js",
    "express": "^4.16.4",
    "fuzzy": "^0.1.3",
    "infobox-parser": "github:dijs/infobox-parser",
    "knex": "0.15.2",
    "node-fetch": "^2.3.0",
    "nodemw": "github:gazmull/nodemw#useTokenJustWhenNeeded",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.0.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/knex": "^0.15.1",
    "@types/node-fetch": "^2.1.4",
    "@types/node": "^10.12.10",
    "@types/pug": "^2.0.4",
    "@types/serve-favicon": "^2.2.30",
    "@types/sqlite3": "^3.1.3",
    "del-cli": "^1.1.0",
    "tslint-config-typings": "^0.3.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "uglifyjs-folder": "^1.5.1"
  }
}
