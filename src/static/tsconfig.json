{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": false,
    "module": "none",
    "incremental": true,
    "target": "es2017",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "outDir": "../../dist/static/js",
    "removeComments": true,
  },
  "include": [
    "ts/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
