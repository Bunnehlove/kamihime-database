- for(let i = 0; i < Eidolons.length; i++) 
    .container(id=Eidolons[i].khID name=Eidolons[i].khName style='display: inline-block;')
        img(class="avatar" src=Eidolons[i].khInfo_avatar)
        span(class="iName" value=Eidolons[i].khName) 
            strong #{Eidolons[i].khName}
        br
        span(class="iID" value=Eidolons[i].khID)
            span(style="font-size:13px") ##{Eidolons[i].khID.toUpperCase()}
        span(class='episode')
            - if(Eidolons[i].khHarem_introFile)
                <a href='#{Eidolons[i].khHarem_introFile}'>Episode 1 (Youtube)</a> | 
            - else
                <a style='color:grey'>Episode 1</a> | 
            <a href='http://kamihimedb.thegzm.space/player/#{Eidolons[i].khID}/2/#{Eidolons[i].khHarem_hentai1Resource2}'>Episode 2</a>